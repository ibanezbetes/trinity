{
  "name": "trinity-ai-dev",
  "version": "1.0.0",
  "description": "Trinity AI Assistant Lambda - Trini chatbot with improved JSON parsing, ChatML protocol, and TMDB verification",
  "main": "ai.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:property": "jest --testPathPattern=property"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.978.0",
    "@aws-sdk/client-dynamodb": "^3.282.0",
    "@aws-sdk/lib-dynamodb": "^3.975.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "openai": "^4.28.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/tests/**",
      "!ai.js",
      "!jest.setup.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
